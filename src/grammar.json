      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "/*"
        },
        {
          "type": "PATTERN",
          "value": "[^*]*\\*+([^/*][^*]*\\*+)*"
        },
        {
          "type": "STRING",
          "value": "/"
        }
      ]
    },
    "doc_comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "/**"
        },
        {
          "type": "PATTERN",
          "value": "[^*]*\\*+([^/*][^*]*\\*+)*"
